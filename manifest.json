{
	"manifest_version": 2,

	"name": "SidebarTimer",
	"description": "Start a timer in the sidebar.",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"]
	},
	"icons": {
		"16": "icon_16.png",
		"48": "icon_48.png",
		"128": "icon_128.png"
	},
	"sidebar_action": {
		"default_icon": "icon.png",
		"default_panel": "panel.html"
	}
}